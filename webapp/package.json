{
  "name": "react-typescript",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/basarat/react-typescript.git"
  },
  "scripts": {
    "clean": "rm -rf ./node_modules package-lock.json && npm install",
    "build:check": "tsc -p .",
    "build:server": "tsc -p ./tsconfig.server.json",
    "build:webpack": "webpack -p",
    "build": "npm run build:check && npm run build:server && npm run build:webpack",
    "start:server": "ts-node-dev src/server/server.ts",
    "start:webpack": "webpack-dev-server -d --content-base ./public",
    "start": "concurrently \"npm run start:server\" \"npm run start:webpack\""
  },
  "dependencies": {
    "@types/express": "4.16.0",
    "@types/lowdb": "1.0.6",
    "@types/react": "16.7.13",
    "@types/react-dom": "16.0.11",
    "axios": "0.21.1",
    "clean-webpack-plugin": "1.0.0",
    "concurrently": "4.1.0",
    "cors": "2.8.5",
    "express": "4.16.4",
    "formstate": "1.0.2",
    "html-webpack-plugin": "3.2.0",
    "lowdb": "1.0.0",
    "mobx": "4.3.1",
    "mobx-react": "5.4.2",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "takeme": "0.11.1",
    "ts-loader": "5.3.1",
    "ts-node-dev": "1.0.0-pre.31",
    "typescript": "3.2.1",
    "typestyle": "2.0.1",
    "uuid": "3.3.2",
    "webpack": "4.27.1",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "^3.1.14"
  }
}
